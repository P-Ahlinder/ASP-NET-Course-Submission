@model IEnumerable<AppUser>


<div class="admin-panel">
<h1>WELCOME SYSTEM ADMIN</h1>
<div class="accounts">

    <div class="admins">
    <h1>Admin accounts</h1>
    @if (User.IsInRole("admin"))
    {
        <p>@User.Identity.Name</p>
    }
    </div>
    
    <div class="users">
        <h1>User accounts</h1>
            @foreach(var item in Model)
            {
              <div class="profile">
                <p>
                    @item.FirstName
                    @item.LastName
                </p>
   
                <p>
                    @item.UserName
                </p>
              </div>
            }
        </div>
    </div>
</div>